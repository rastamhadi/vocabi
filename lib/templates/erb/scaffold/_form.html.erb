<%%= simple_form_for @<%= model_resource_name %> do |f| %>
  <%- attributes.each do |attribute| -%>
  <%%= f.input :<%= attribute.column_name %> %>
  <%- end -%>
  <%%= f.button :submit, class: 'btn-primary' %>
  <%%= link_to t('helpers.links.cancel'), <%= index_helper %>_path, class: 'btn btn-default' %>
<%% end %>
